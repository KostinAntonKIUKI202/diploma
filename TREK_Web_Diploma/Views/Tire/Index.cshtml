@using TREK_Web_Diploma.Controllers.spares.sparesWheelset;
@using TREK_Web_Diploma.Models.spares.sparesWheelset;
@model IEnumerable<Tire>

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<link rel="stylesheet" href="~/css/tableViews.css" />
<h1>Pedals</h1>
<table>
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Description</th>
        <th>Quantity</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.TireId</td>
            <td class="text_row row_2">@item.TireName</td>
            <td>@item.TireDescription</td>
            <td class="text_row row_2">@item.TireQuantity</td>
        </tr>
    }
</table>