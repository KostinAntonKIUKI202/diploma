@using TREK_Web_Diploma.Controllers.spares.sparesWheelset;
@using TREK_Web_Diploma.Models.spares.sparesWheelset;
@model IEnumerable<Rim>

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<link rel="stylesheet" href="~/css/tableViews.css" />
<h1>Обод</h1>
<table>
    <tr>
        <th>ID</th>
        <th>Назва</th>
        <th>Опис</th>
        <th>Кількість</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.RimId</td>
            <td class="text_row row_2">@item.RimName</td>
            <td>@item.RimDescription</td>
            <td class="text_row row_2">@item.RimQuantity</td>
        </tr>
    }
</table>
