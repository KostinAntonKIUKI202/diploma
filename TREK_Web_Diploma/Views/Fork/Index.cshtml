@using TREK_Web_Diploma.Controllers.spares.sparesFrameset;
@using TREK_Web_Diploma.Models.spares.sparesFrameset;
@model IEnumerable<Fork>

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<link rel="stylesheet" href="~/css/tableViews.css" />
<h1>Вилки</h1>
<table>
    <tr>
        <th>ID</th>
        <th>Назва</th>
        <th>Опис</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ForkId</td>
            <td class="text_row row_2">@item.ForkName</td>
            <td>@item.ForkDescription</td>
        </tr>
    }
</table>