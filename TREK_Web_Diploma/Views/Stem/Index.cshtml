@using TREK_Web_Diploma.Controllers.spares.sparesEquipment;
@using TREK_Web_Diploma.Models.spares.sparesEquipment;
@model IEnumerable<Stem>

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<link rel="stylesheet" href="~/css/tableViews.css" />
<h1>Stem</h1>
<table>
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Quantity</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.StemId</td>
            <td class="text_row row_2">@item.StemName</td>
            <td>@item.StemQuantity</td>
        </tr>
    }
</table>