@{
}
<link rel="icon" href="~/favicon.ico" />
<link rel="stylesheet" href="~/css/tableViews.css" />
<h1>Staff</h1>
<table>
    <tr>
        <th>№</th>
        <th class="text_row row_2">Ім'я</th>
        <th class="text_row">Фамілія</th>
    </tr>
    @foreach(var item in Model)
    {
    <tr>
        <td>@item.StaffId</td>
        <td class="text_row row_2">@item.FirstName</td>
        <td class ="text_row">@item.SecondName</td>
    </tr>
    }
</table>