@using TREK_Web_Diploma.Controllers.production;
@using TREK_Web_Diploma.Models.production;
@model IEnumerable<Wheelset>

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<link rel="stylesheet" href="~/css/tableViews.css" />
<h1>Вілсети</h1>
<table>
    <tr>
        <th>ID</th>
        <th>Втулка</th>
        <th>Обід</th>
        <th>Покришка</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.WheelsetId</td>
            <td class="text_row row_2">@item.Hub.HubName</td>
            <td>@item.Rim.RimName</td>
            <td class="row_2">@item.Tire.TireName</td>
        </tr>
    }
</table>