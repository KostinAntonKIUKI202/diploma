@using TREK_Web_Diploma.Controllers.spares.sparesGroopset;
@using TREK_Web_Diploma.Models.spares.sparesGroopset;
@model IEnumerable<Transmition>

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<link rel="stylesheet" href="~/css/tableViews.css" />
<h1>Трансмісія</h1>
<table>
    <tr>
        <th>ID</th>
        <th>Манетка</th>
        <th>Перемикач</th>
        <th>Система</th>
        <th>Касета</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.TransmitionId</td>
            <td class="text_row row_2">@item.Shifter.ShifterName</td>
            <td>@item.Switch.SwitchName</td>
            <td class="row_2">@item.FrontGear.FrontGearName</td>
            <td>@item.Cassette.CassetteName</td>
        </tr>
    }
</table>