@using TREK_Web_Diploma.Controllers.spares.sparesWheelset;
@using TREK_Web_Diploma.Models.spares.sparesWheelset;
@model IEnumerable<Hub>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<link rel="stylesheet" href="~/css/tableViews.css" />
<h1>Hub</h1>
<table>
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Description</th>
        <th>Quantity</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.HubId</td>
            <td class="text_row row_2">@item.HubName</td>
            <td>@item.HubDescription</td>
            <td class="row_2"></td>
        </tr>
    }
</table>